{
  "env": {
    "node": true
  },
  "extends": ["@synatic/eslint-config-synatic-node"],
  "globals": {},
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "overrides": [],
  "rules": {
    "prettier/prettier": "warn",
    "sonarjs/prefer-single-boolean-return": "warn",
    "no-prototype-builtins": "warn",
    "curly": ["error", "all"],
    "sonarjs/no-duplicated-branches": "warn"
  }
}
